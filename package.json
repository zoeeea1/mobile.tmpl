{
    "name": "webpack-demo",
    "version": "1.0.0",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "webpack-dev-server --watch --useLocalIp --open --config ./build/webpack.config.js",
        "build": "npm run clean && webpack --env.prod --hide-modules  --progress --colors --config ./build/webpack.config.js",
        "build:test": "npm run clean && webpack --env.test --hide-modules  --progress --colors --config ./build/webpack.config.js"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "babel-polyfill": "^6.26.0",
        "draggabilly": "^2.1.1",
        "exif-js": "^2.3.0",
        "object-assign": "^4.1.0",
        "vue": "^2.3.4",
        "vue-awesome-swiper": "^3.1.3",
        "vue-carousel-3d": "0.1.9",
        "vue-clipboard2": "^0.2.0",
        "vue-lazyload": "^1.0.6",
        "vue-router": "^2.7.0",
        "vue2-datepick": "^3.0.10",
        "vuex": "^3.0.0"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "clean-webpack-plugin": "^0.1.16",
        "css-loader": "^0.28.4",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.29.0",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "style-loader": "^0.18.2",
        "rimraf": "^2.6.2",
        "url-loader": "^0.5.9",
        "vue-loader": "^13.0.1",
        "vue-template-compiler": "^2.3.4",
        "webpack": "^3.1.0",
        "webpack-dev-server": "^2.5.1"
    }
}